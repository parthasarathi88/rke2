apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: jaeger
  namespace: logging-tracing
  annotations:
    kubernetes.io/ingress.class: kong-external
spec:
  rules:
  - host: jaeger.dellpc.in
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: jaeger-query
            port:
              number: 80